# Uncomment if you want to ignore them
# package-lock.json
# yarn.lock
# composer.lock

# Runtime Files
*.pid
*.sock
*.lock

# Cache Directories
.cache/
cache/
.npm/
.yarn/

# Build Artifacts
build/
dist/
out/

# Local Environment Overrides
docker-compose.override.yml
docker-compose.local.yml

# Deployment Specific
deployment-config.local.yml
inventory.local.yml

# Performance and Profiling
*.prof
*.trace

# Documentation Drafts
docs/drafts/
*.draft.md

