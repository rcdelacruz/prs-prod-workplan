*.tfstate
*.tfstate.*
.terraform/
.terraform.lock.hcl

# Ansible (if used for configuration management)
*.retry
.vault_pass

# Kubernetes (if used for orchestration)
*.kubeconfig
kube-config

# Helm (if used for Kubernetes deployments)
charts/*/charts/
charts/*/requirements.lock

# Local Development Overrides
.local/
local/

# Ignore compiled documentation
docs/docs/.venv/
docs/docs/node_modules/

# Ignore MkDocs build artifacts
docs/site/

# Ignore any local customizations
*-local.*
*-custom.*
